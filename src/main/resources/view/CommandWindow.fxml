<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="Infinity" maxWidth="Infinity" minHeight="-Infinity"
            minWidth="-Infinity"
            prefHeight="600.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1">
    <TextArea fx:id="inputTextField" layoutY="558.0" prefHeight="120.0" prefWidth="400.0"
              promptText="Enter command here" wrapText="true" AnchorPane.bottomAnchor="1.0">
        <font>
            <Font size="13.0"/>
        </font>
    </TextArea>
    <ScrollPane fx:id="scrollPane" hbarPolicy="NEVER" maxHeight="1.7976931348623157E308" maxWidth="400.0"
                prefHeight="480.0" prefWidth="400.0" vvalue="1.0">
        <VBox fx:id="messageContainer" maxWidth="400.0" minWidth="400.0" prefWidth="400.0"/>
    </ScrollPane>
</AnchorPane>
