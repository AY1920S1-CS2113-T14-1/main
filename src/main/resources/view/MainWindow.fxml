<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TabPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.Scene?>
<?import javafx.stage.Stage?>

<fx:root xmlns:fx="http://javafx.com/fxml/1" minHeight="600" minWidth="1000" title="Dr. Duke" type="javafx.stage.Stage"
         resizable="false" xmlns="http://javafx.com/javafx/11.0.1">
    <icons>
        <Image url="@/images/icon.png"/>
    </icons>
    <scene>
        <Scene>
            <BorderPane prefHeight="600.0" prefWidth="1073.0">
                <right>
                    <AnchorPane fx:id="commandWindowHolder" prefHeight="600.0" prefWidth="400.0"
                                BorderPane.alignment="CENTER"/>
                </right>
                <center>
                    <TabPane fx:id="contextWindowHolder" prefHeight="600.0" prefWidth="673.0"
                             tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
                    </TabPane>
                </center>
            </BorderPane>
        </Scene>
    </scene>
</fx:root>
