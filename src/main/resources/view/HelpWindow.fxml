<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>

<StackPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
           minWidth="-Infinity"
           prefHeight="600.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1">
    <children>
        <JFXListView fx:id="helpListView" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                     minWidth="-Infinity" prefHeight="600.0" prefWidth="400.0"/>
        <AnchorPane fx:id="emptyPane" prefHeight="200.0" prefWidth="200.0" visible="false">
            <children>
                <ImageView fitHeight="200.0" fitWidth="200.0" layoutX="100.0" layoutY="200.0" pickOnBounds="true"
                           preserveRatio="true">
                    <image>
                        <Image url="@../images/404.png"/>
                    </image>
                </ImageView>
            </children>
        </AnchorPane>
    </children>
